{
  "name": "@candle-analytics/rrweb-all",
  "version": "2.0.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "rrweb",
    "@candle-analytics/rrweb-all"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "yarn turbo run prepublish",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "check-types": "tsc -noEmit",
    "prepublish": "tsc -noEmit && vite build",
    "lint": "yarn eslint src/**/*.ts"
  },
  "homepage": "https://github.com/candle-analytics/rrweb/tree/main/packages/@rrweb/all#readme",
  "bugs": {
    "url": "https://github.com/candle-analytics/rrweb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/candle-analytics/rrweb.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/rrweb-all.cjs",
  "module": "./dist/rrweb-all.js",
  "unpkg": "./dist/rrweb-all.umd.cjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rrweb-all.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/rrweb-all.cjs"
      }
    }
  },
  "files": [
    "build",
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "puppeteer": "^20.9.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.4.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@candle-analytics/rrweb-types": "^2.0.0",
    "@candle-analytics/rrweb-packer": "^2.0.0",
    "@candle-analytics/rrweb": "^2.0.0"
  },
  "browserslist": [
    "supports es6-class"
  ]
}
